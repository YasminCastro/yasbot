{
  "name": "yasbot",
  "version": "1.0.0",
  "description": "Bot de WhatsApp usando whatsapp-web.js",
  "type": "commonjs",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --quiet src/index.ts",
    "start:prod": "yarn build && pm2 start dist/index.js --name yasbot --env production",
    "restart:prod": "yarn build && pm2 stop yasbot && pm2 start dist/index.js --name yasbot --env production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/qrcode-terminal": "^0.12.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "mongodb": "^6.16.0",
    "node-cron": "^4.1.0",
    "puppeteer": "^24.9.0",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.31.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
